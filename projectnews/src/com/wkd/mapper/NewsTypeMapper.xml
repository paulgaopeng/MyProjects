<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.wkd.mapper.NewsTypeMapper">
	
	<select id="queryNewsTypeList" 
		resultType="com.wkd.entity.NewsType">
		select type_id typeId,type_name typeName,type_descr typeDescr
		from news_type
	</select>
	
</mapper>